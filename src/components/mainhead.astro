---
export interface Props {
    title: string;
}

const { title } = Astro.props;
---

<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width" />
<link rel="icon" type="image/svg+xml" href="/images/sukhfx.svg" />
<meta name="robots" content="index,follow">
<meta name="description" content="My personal portfolio website. You can see my skills and can connect with me.">
<meta name="author" content="SukhFX">
<link rel="canonical" href="https://sukhop.netlify.app/">
<meta property="og:title" content={title}>
<meta property="og:description" content="Here it is my personal website. I sometimes share posts and articles about things that I am interested in.">
<meta property="og:locale" content="en-IN" />
<meta property="og:type" content="website" />
<meta property="og:url" content="https://sukhop.netlify.app/"/>
<meta property="og:image" content="https://sukhop.netlify.app/images/sukhfx.svg">
<meta name="keywords" content="sukhfx, sukhop, sukhbir singh, web designer, graphic designer, social media designer, freelancer, web developer, programmer">
<meta name="google-site-verification" content="w3jfp1HMCY8zH4pERNL7J2Lv0_r7sTLcYXCiZUvx4ZY" />

<!-- bootstrap -->
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous" />

<!-- Iconscout Link -->
<link data-n-head="ssr" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css" rel="stylesheet">
<link data-n-head="ssr" href="https://unicons.iconscout.com/release-pro/v4.0.0/css/solid.css" rel="stylesheet">
<link data-n-head="ssr" href="https://unicons.iconscout.com/release-pro/v4.0.0/css/thinline.css" rel="stylesheet">
<script data-n-head="ssr" src="https://unicons.iconscout.com/release-pro/v4.0.0/script/monochrome/bundle.js" async=""></script>

<!-- Fontawesome Link -->
<link rel="stylesheet" href="https://site-assets.fontawesome.com/releases/v6.1.2/css/all.css">

<title>{title}</title>
